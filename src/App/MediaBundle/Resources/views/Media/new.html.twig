{% extends 'AppAdminBundle:Layout:admin.html.twig' %}
{% form_theme form 'AppAdminBundle:Form:fields.html.twig' %}

{% block stylesheets %}

    <link href="{{ asset('bundles/appadmin/css/assets/bootstrap-fileupload/bootstrap-fileupload.css') }}" rel="stylesheet">
    {{ parent() }}
{% endblock %}

{% block body %}
    <div class="row">
        <div class="col-lg-12">
            <section class="panel">
                <header class="panel-heading">
                    Création d'un média
                </header>
                {{ form(form) }}
            </section>
        </div>
    </div>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('bundles/appadmin/js/assets/bootstrap-fileupload/bootstrap-fileupload.js') }}" ></script>
{% endblock %}
