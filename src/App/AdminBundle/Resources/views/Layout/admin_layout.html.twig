<!DOCTYPE html>
<html lang="fr">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="img/favicon.png">

    <title>BackOffice</title>

    {% block stylesheets %} 
    <!-- Bootstrap core CSS -->
      <link href="{{ asset('bundles/appadmin/css/bootstrap.min.css') }}" rel="stylesheet">
      <link href="{{ asset('bundles/appadmin/css/bootstrap-reset.css') }}" rel="stylesheet">
      <link href="{{ asset('bundles/appadmin/css/assets/font-awesome/css/font-awesome.css') }}" rel="stylesheet">
      <link href="{{ asset('bundles/appadmin/css/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.css') }}" rel="stylesheet">
      <link href="{{ asset('bundles/appadmin/css/owl.carousel.css') }}" rel="stylesheet">
      <link href="{{ asset('bundles/appadmin/css/style.css') }}" rel="stylesheet">
      <link href="{{ asset('bundles/appadmin/css/style-responsive.css') }}" rel="stylesheet">
      <link href="{{ asset('bundles/appadmin/css/assets/jquery-multi-select/css/multi-select.css') }}" rel="stylesheet"/>
      <link href="{{ asset('bundles/appadmin/css/assets/advanced-datatable/media/css/demo_table.css') }}" rel="stylesheet" />
      <link href="{{ asset('bundles/appadmin/css/assets/bootstrap-wysihtml5/bootstrap-wysihtml5.css') }}" rel="stylesheet" />
      <link href="{{ asset('bundles/appadmin/js/jcrop/css/jquery.Jcrop.min.css') }}" rel="stylesheet" />
      
   {% endblock %}

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 tooltipss and media queries -->
    <!--[if lt IE 9]>
      <script src="js/html5shiv.js"></script>
      <script src="js/respond.min.js"></script>
    <![endif]-->
  </head>
  <body>
  <section id="container" class="">
      <!--header start-->
      <header class="header white-bg">
              <div class="sidebar-toggle-box">
                  <div class="fa fa-bars tooltips" data-placement="right" data-original-title="Toggle Navigation"></div>
              </div>
            <!--logo start-->
            <a href="/" class="logo">Back<span>Office</span></a>
            <!--logo end-->

            <div class="top-nav ">
                <!--search & user info start-->
                <ul class="nav pull-right top-menu">
                    <li>
                        <input type="text" class="form-control search" placeholder="Search">
                    </li>
                    <!-- user login dropdown start-->
                    <li class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                            <img alt="" src="">
                            <span class="username">{% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}{{ app.user.username }}{% endif %}</span>
                            <b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu extended logout">
                            <div class="log-arrow-up"></div>
                            <li><a href="{{ path('fos_user_security_logout') }}"><i class="fa fa-key"></i> Se d√©conneter</a></li>
                        </ul>
                    </li>
                    <!-- user login dropdown end -->
                </ul>
                <!--search & user info end-->
            </div>
        </header>
      {% include "AppAdminBundle:Menu:main_menu.html.twig" %}
      <section id="main-content">
          <section class="wrapper site-min-height">
            
            {% block body %}
            {% endblock %}
          </section>
      </section>

      <footer class="site-footer">
          <div class="text-center">
              2014 &copy; BackOffice
              <a href="#" class="go-top">
                  <i class="fa fa-angle-up"></i>
              </a>
          </div>
      </footer>
      <!--footer end-->
  </section>
{% block javascripts %} 
    <!-- js placed at the end of the document so the pages load faster -->

    <!-- --- JQUERY --- -->
    
    <script src="{{ asset('bundles/appadmin/js/jquery-1.8.3.min.js') }}" ></script>

    <!-- --- JQUERY PLUGINS --- -->

    <script src="{{ asset('bundles/appadmin/js/jquery.nicescroll.js') }}" ></script>
    <script src="{{ asset('bundles/appadmin/js/jquery.scrollTo.min.js') }}" ></script>
    <script src="{{ asset('bundles/appadmin/js/jquery.sparkline.js') }}" ></script>
    <script src="{{ asset('bundles/appadmin/js/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.js') }}" ></script>
    <script src="{{ asset('bundles/appadmin/js/jquery.customSelect.min.js') }}" ></script>
    <script src="{{ asset('bundles/appadmin/js/assets/jquery-multi-select/js/jquery.multi-select.js') }}"></script>

    <!-- --- BOOTSTRAP --- -->

    <script src="{{ asset('bundles/appadmin/js/bootstrap.min.js') }}" ></script>
    <script src="{{ asset('bundles/appadmin/js/bootstrap-switch.js') }}" ></script>

    <!-- --- WYSIYYG HTML5 --- -->

    <script src="{{ asset('bundles/appadmin/js/assets/bootstrap-wysihtml5/wysihtml5-0.3.0.js') }}"></script>
    <script src="{{ asset('bundles/appadmin/js/assets/bootstrap-wysihtml5/bootstrap-wysihtml5.js') }}"></script>

    <!-- --- DATATABLE --- -->

    <script src="{{ asset('bundles/appadmin/js/assets/data-tables/jquery.dataTables.js') }}" ></script>
    <script src="{{ asset('bundles/appadmin/js/assets/data-tables/DT_bootstrap.js') }}" ></script>
    <script src="{{ asset('bundles/appadmin/js/assets/advanced-datatable/media/js/jquery.dataTables.js') }}"></script>
    <script src="{{ asset('bundles/appadmin/js/assets/advanced-datatable/examples/examples_support/jquery.tooltip.js') }}" ></script>

    <!-- --- OTHER --- -->

    <script src="{{ asset('bundles/appadmin/js/owl.carousel.js') }}" ></script>
    <script src="{{ asset('bundles/appadmin/js/sparkline-chart.js') }}" ></script>
    <script src="{{ asset('bundles/appadmin/js/easy-pie-chart.js') }}" ></script>
    <script src="{{ asset('bundles/appadmin/js/count.js') }}" ></script>
    <script src="{{ asset('bundles/appadmin/js/common-scripts.js') }}" ></script>
    <script src="{{ asset('bundles/appadmin/js/assets/fuelux/js/spinner.min.js') }}"></script>
    
    <script src="{{ asset('bundles/appadmin/js/jcrop/js/jquery.Jcrop.min.js') }}"></script>
    
    <script class="include" type="text/javascript" src="{{ asset('bundles/appadmin/js/jquery.dcjqaccordion.2.7.js') }}" ></script>

    <script>
     var Script = function () { $('#spinner4').spinner({value:0, step: 1, min: 0, max: 200000});}();
    </script>
    

{% endblock %}
<!-- END JAVASCRIPTS -->
  </body>
</html>
